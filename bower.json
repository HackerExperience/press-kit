{
  "name": "he2_press_kit",
  "description": "Hacker Experience 2 Press Kit",
  "private": true,
  "devDependencies": {
    "bootstrap-sass": "3.3.6",
    "font-awesome-sass": "4.5.0",
    "jquery": "2.2.0",
    "lightbox2": "*"
  },
  "exportsOverride": {
    "bootstrap-sass": {
      "sass": "assets/stylesheets/**"
    },
    "font-awesome-sass": {
      "sass": "assets/stylesheets/**",
      "font": "assets/fonts/**"
    },
    "jquery": {
      "js": "dist/jquery.min.js"
    },
    "lightbox2": {
      "js": "dist/js/lightbox.min.js",
      "img": "dist/images/**"
    },
    "*": {}}}
